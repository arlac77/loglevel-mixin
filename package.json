{
  "name": "loglevel-mixin",
  "version": "0.0.0-semantic-release",
  "description": "mixin to declare logging methods named after a set of log levels",
  "main": "dist/LogLevelMixin.js",
  "module": "src/LogLevelMixin.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "build": "node_modules/rollup/bin/rollup --output=dist/LogLevelMixin.js -c -- src/LogLevelMixin.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/loglevel-mixin.git"
  },
  "engines": {
    "node": ">=6.9.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "2.11.15",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.38.0",
    "semantic-release": "^6.3.5"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "keywords": [
    "loglevel",
    "logging"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/loglevel-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/loglevel-mixin#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ]
}