{
  "name": "loglevel-mixin",
  "version": "0.0.0-semantic-release",
  "description": "mixin to declare logging methods named after a set of log levels",
  "main": "dist/LogLevelMixin.js",
  "module": "src/LogLevelMixin.js",
  "scripts": {
    "cover": "nyc ava",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/LogLevelMixin.js -c -- src/LogLevelMixin.js",
    "pretest": "rollup -c rollup.config.test.js",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/loglevel-mixin.git"
  },
  "engines": {
    "node": ">=7.7.2"
  },
  "devDependencies": {
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.41.5",
    "semantic-release": "^6.3.6",
    "nyc": "^10.1.2",
    "ava": "^0.18.2",
    "babel-preset-es2015-rollup": "^3.0.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "keywords": [
    "loglevel",
    "logging"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/loglevel-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/loglevel-mixin#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}