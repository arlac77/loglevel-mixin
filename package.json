{
  "name": "loglevel-mixin",
  "version": "0.0.0-semantic-release",
  "description": "mixin to declare logging methods named after a set of log levels",
  "main": "dist/LogLevelMixin.js",
  "jsnext:main": "src/LogLevelMixin.js",
  "module": "src/LogLevelMixin.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "jsdoc src/*.js",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/LogLevelMixin.js -- src/LogLevelMixin.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/loglevel-mixin.git"
  },
  "engines": {
    "node": ">=6.5"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "cracks": "3.1.2",
    "markdown-doctest": "^0.8.1",
    "cz-conventional-changelog": "1.2.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.4.1",
    "mocha": "3.0.2",
    "rollup": "^0.36.0",
    "semantic-release": "6.3.0"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "keywords": [
    "loglevel",
    "logging"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/loglevel-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/loglevel-mixin#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
