{
  "name": "loglevel-mixin",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/loglevel-mixin.mjs",
  "module": "src/loglevel-mixin.mjs",
  "description": "mixin to declare logging methods named after a set of log levels",
  "keywords": [
    "loglevel",
    "logging"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/loglevel-mixin.mjs --section=API",
    "lint": "documentation lint src/loglevel-mixin.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.0.0",
    "c8": "^5.0.1",
    "documentation": "^11.0.0",
    "esm": "^3.2.25",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.14"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/loglevel-mixin.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/loglevel-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/loglevel-mixin#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "extensions": [
      "mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
