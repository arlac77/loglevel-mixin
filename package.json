{
  "name": "loglevel-mixin",
  "version": "0.0.0-semantic-release",
  "description": "mixin to declare logging methods named after a set of log levels",
  "main": "dist/LogLevelMixin.js",
  "module": "src/LogLevelMixin.js",
  "scripts": {
    "cover": "nyc npm test",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/LogLevelMixin.js -c -- src/LogLevelMixin.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/loglevel-mixin.git"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "cracks": "3.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.4.2",
    "nyc": "^10.3.2",
    "rollup": "^0.41.6",
    "semantic-release": "^6.3.6"
  },
  "release": {
    "verifyRelease": "cracks"
  },
  "keywords": [
    "loglevel",
    "logging"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/loglevel-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/loglevel-mixin#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}